// Codigo para Tinkerkat
#include <Adafruit_NeoPixel.h>
#include <string.h>

const byte neopixelPin = 10; 
int y = 0;
int x=255;
int Arreglo[10][10][3];
String mat="50-10-10-10-10-10-10-10-10-19-";
Adafruit_NeoPixel neopixel = Adafruit_NeoPixel(100, neopixelPin, NEO_GRB + NEO_KHZ800);


void setup() {
  Serial.begin(9600);
  neopixel.begin();  
  leer_mat();
  
  
}

void loop() {    
  
}

void ImpMat(){
  for(int i=0;i<20;i++){    
    for(int j=0;j<100;j++){
      	if(j%2==0)  neopixel.setPixelColor(j, x, 0, y); 
      	else  neopixel.setPixelColor(j, y, 0, x); 
            	  
    }
    neopixel.show();        
    delay(1000);
  	x-=10;
  	y+=10;
  }   
  neopixel.clear(); 
  neopixel.show();
  delay(100);     
}

void leer_mat(){
  unsigned int c=0;
  String value, Color[3];
  unsigned int color[100][3];
  for(int i=0;i<300;){
    for(int x=0;;x++){
      if(mat[c+x]=='/'){
        for(unsigned int d=0;;){
          if(mat[c+x+d]!='-') Color[d]+=;
          
        }
        Serial.println(value);
        value="";
        i++;
        c+=x+1;
        break;
      }
      else{
        value+=mat[c+x];
      }
    }
    
  }
  
}